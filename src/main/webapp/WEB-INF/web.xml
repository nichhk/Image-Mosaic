<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
<!-- <web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"> -->



        <servlet>
            <servlet-name>makeMosaic</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.MakeMosaicServlet</servlet-class>
        </servlet>

        <servlet>
            <servlet-name>crawl</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.CrawlerServlet</servlet-class>
        </servlet>

        <servlet>
            <servlet-name>home</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.HomeServlet</servlet-class>
        </servlet>

        <servlet>
            <servlet-name>checkIndex</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.CheckIndexServlet</servlet-class>
        </servlet>

        <servlet>
            <servlet-name>populateIndex</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.PopulateIndexServlet</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>contact</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.ContactServlet</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>gettingStarted</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.GettingStartedServlet</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>getMosaic</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.GetMosaicServlet</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>upload</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.UploadServlet</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>gettingStartedInteractive</servlet-name>
            <servlet-class>com.google.appengine.demos.ImageCollage.GettingStartedInteractiveServlet</servlet-class>
        </servlet>


        <servlet-mapping>
            <servlet-name>gettingStartedInteractive</servlet-name>
            <url-pattern>/getting_started_interactive</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>makeMosaic</servlet-name>
            <url-pattern>/make_mosaic</url-pattern>
        </servlet-mapping>

        <servlet-mapping>
            <servlet-name>crawl</servlet-name>
            <url-pattern>/crawl</url-pattern>
        </servlet-mapping>

        <servlet-mapping>
            <servlet-name>home</servlet-name>
            <url-pattern>/</url-pattern>
        </servlet-mapping>

        <servlet-mapping>
            <servlet-name>checkIndex</servlet-name>
            <url-pattern>/check_index</url-pattern>
        </servlet-mapping>

        <servlet-mapping>
            <servlet-name>populateIndex</servlet-name>
            <url-pattern>/populate_index</url-pattern>
        </servlet-mapping>

        <servlet-mapping>
            <servlet-name>contact</servlet-name>
            <url-pattern>/contact</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>gettingStarted</servlet-name>
            <url-pattern>/getting_started</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>getMosaic</servlet-name>
            <url-pattern>/get_mosaic</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>upload</servlet-name>
            <url-pattern>/upload</url-pattern>
        </servlet-mapping>

    <welcome-file-list>
            <welcome-file>home</welcome-file>
        </welcome-file-list>
    </web-app>
